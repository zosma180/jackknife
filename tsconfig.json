{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es5", "dom", "es2015.iterable", "es2015.promise" ],
    "module": "commonjs",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true
  }
}
