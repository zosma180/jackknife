{
  "name": "jackknife",
  "version": "4.0.1",
  "description": "The useful compact tool.",
  "homepage": "https://github.com/zosma180/jackknife",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zosma180/jackknife.git"
  },
  "types": "./dist/mjs/index.d.ts",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build-mjs": "npx tsc",
    "build-cjs": "npx tsc --project tsconfig.cjs.json",
    "build": "npm run clean && npm run lint && npm run build-mjs && npm run build-cjs",
    "deploy": "npm-deploy.mjs . .",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts*\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "jackknife",
    "tool",
    "util",
    "utility",
    "function",
    "math",
    "ts",
    "typescript"
  ],
  "author": "Luca Morello",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.4.0 ",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3",
    "uglify-js": "^3.17.4"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README",
    "DISCLAIMER",
    "dist"
  ]
}
